    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC4vqrgTZ81rE5juLYbXT6Tg7y6T8Ii0Nc&amp;callback=initMap&amp;language=ru"></script>
    <script>
        var landImage = "{{ landingImage }}/map-marker.png";

        function initMap() {
            var uluru = {lat: 50.421125, lng: 30.523122};
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 15,
                center: uluru,
                disableDefaultUI: true,
                scrollwheel: false
            });

            var title = 'г. Киев, ул. Предславинская, 37, оф. 413, 4 этаж';

            var contentString = '<div class="contact__info-window">' + title + '</div>';

            var infowindow = new google.maps.InfoWindow({
                content: contentString
            });

            var marker = new google.maps.Marker({
                position: uluru,
                map: map,
                title: title,
                icon: landImage
            });

            infowindow.open(map, marker);
        }
    </script>
